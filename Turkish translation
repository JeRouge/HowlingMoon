# Commands
##############################
commands.werewolf.success=%s artık kurt adam
commands.werewolf.success.false=%s artık kurt adam değil
commands.werewolf.usage=/werewolf <true|false>

commands.werelevel.failure=Kurt adam olmayan oyuncunun seviyesi ayarlanamadı.
commands.werelevel.failure.levels=Kurt adam seviyesi 1'den aşağı ayarlanama.
commands.werelevel.success=Kurt adam seviyesini %s yap %d için
commands.werelevel.usage=/werelevel <amount> ya da /werelevels <amount>

commands.wereprogress.failure=Gelişim 0'dan az a ayarlanamaz.
commands.wereprogress.failure.non=Kurt adam olmayan oyuncu gelişimini ayarlayamaz.
commands.wereprogress.success=Kurt adam gelşimini (%d, %s) yap %s için
commands.wereprogress.usage=/wereprogress <0-8> [-1, 0 , 1] ya da /werequests <0-8> [-1, 0, 1]

